{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter: Attach to Device",
      "type": "dart",
      "request": "attach"
    },
    {
      "name": "Run Current Test",
      "type": "dart",
      "request": "launch",
      "program": "${file}",
      "args": [
        "--coverage"
      ],
    },
    {
      "name": "mobile_network_evaluator",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "mobile_network_evaluator (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "mobile_network_evaluator (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}